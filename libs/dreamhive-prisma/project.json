{
  "name": "dreamhive-prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dreamhive-prisma",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project dreamhive-prisma --web",
  "targets": {
    "prisma": {
      "command": "prisma init --datasource-provider mongodb",
      "options": {
        "cwd": "libs/dreamhive-prisma"
      }
    },
    "push": {
      "command": "prisma db push",
      "options": {
        "cwd": "libs/dreamhive-prisma"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/dreamhive-prisma"
      }
    }
  }
}
